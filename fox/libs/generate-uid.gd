# ------------------------------------------------------------------------------

extends Node

# ------------------------------------------------------------------------------

class_name generateUID

# ------------------------------------------------------------------------------

static func withPrefix(prefix):
  var _prefix = ''
  if (prefix):
    _prefix = prefix + '-'

  return _prefix + str(Time.get_unix_time_from_system()) + '-' + str(Time.get_ticks_msec()) + '-' + str(randi() % 900000 + 100000)

