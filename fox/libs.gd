
# ------------------------------------------------------------------------------

extends Node

# ------------------------------------------------------------------------------

class_name L

# ------------------------------------------------------------------------------

static func generateUID(prefix):
  var _prefix = ''
  if (prefix):
    _prefix = prefix + '-'

  return _prefix + str(OS.get_unix_time()) + '-' + str(OS.get_ticks_msec()) + '-' + str(randi() % 900000 + 100000)

